{
  "expo": {
    "name": "Auto Spotreba",
    "slug": "autoSpotreba",
    "scheme": "auto-spotreba",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#161622",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      },
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#161622"
      },
      "package": "com.patrik1502.autoSpotreba",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe6812c8-3811-4d08-9c82-cda93cd14155"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5616098433053869~7271216168"
  }
}
